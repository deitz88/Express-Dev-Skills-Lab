<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../public/stylesheets/style.css">
    <title>Skills!!</title>
</head>
<body>
    <body>
      <h1>Current Skills as of <%= time %></h1>
        <ul>
          <h3>
          <a href="/skills/new">Add New Skill</a></h3>
          <% skills.forEach(function(skill) { %>
            <li>
             
                <a href="/skills/<%= skill.id %>"><%=skill.skill + ',' %></a>
              <%= 'Currently Fluent? ' + (skill.hasSkill ? 'Yes' : 'No') %>
            </li>
          <% }); %>
        </ul>
      </body>
</body>
</html>